<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="user">
  <template name="seq" value="$CALLER$-&gt;+$CALLEE$: $METHOD$&#10;    $END$&#10;$CALLEE$-&gt;-$CALLER$:" description="Create a sequence block for web sequence diagram" toReformat="false" toShortenFQNames="true">
    <variable name="CALLER" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CALLEE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="METHOD" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="seqopt" value="opt $CONDITION$&#10;    $END$&#10;end" description="opt block for seq" toReformat="false" toShortenFQNames="true">
    <variable name="CONDITION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
  <template name="seqalt" value="alt $CONDITION1$&#10;    $END$&#10;else $CONDITION2$&#10;    &#10;end" description="alternate seq" toReformat="false" toShortenFQNames="true">
    <variable name="CONDITION1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="CONDITION2" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="OTHER" value="true" />
    </context>
  </template>
</templateSet>

